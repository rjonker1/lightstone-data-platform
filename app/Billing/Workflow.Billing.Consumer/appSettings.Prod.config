<?xml version="1.0" encoding="utf-8" ?>
<!-- For more information on using transformations 
     see the web.config examples at http://go.microsoft.com/fwlink/?LinkId=214134. -->
<appSettings xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <appSettings>
    <add key="service/config/name" value="#{Billing_Service_Name}" xdt:Locator="Match(key)" xdt:Transform="Replace"/>
    <add key="service/config/displayName" value="#{Billing_Service_DisaplyName}" xdt:Locator="Match(key)" xdt:Transform="Replace"/>
    <add key="service/config/description" value="#{Billing_Service_Description}" xdt:Locator="Match(key)" xdt:Transform="Replace"/>
    <add key="ClientSettingsProvider.ServiceUri" value="" />
    <add key="rabbitMQ/config/subscriptionPrefix" value="BILLING"/>
  </appSettings>
</appSettings>