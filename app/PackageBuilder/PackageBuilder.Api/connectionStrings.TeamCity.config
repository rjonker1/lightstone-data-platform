<?xml version="1.0" encoding="utf-8" ?>
<!-- For more information on using transformations 
     see the web.config examples at http://go.microsoft.com/fwlink/?LinkId=214134. -->
<connectionStrings xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">

  <add name="RecordsDb" connectionString="db.contoso.com;Initial Catalog=RecordsDb-RELEASE;Integrated Security=true"
         xdt:Transform="Replace" xdt:Locator="Match(name)"/>
  <add name="InventoryDb" connectionString="db.constos.com;Initial Catalog=InventoryDb-RELEASE;Integrated Security=true"
       xdt:Transform="Insert" xdt:Locator="Match(name)" />

  <add name="packageBuilder"
       connectionString="Data Source=.;Database=PackageBuilder;Integrated Security=SSPI;"
       providerName="System.Data.SqlClient" xdt:Transform="Replace" xdt:Locator="Match(name)"/>
  <add name="packageBuilderCommandStore"
      connectionString="Data Source=.;Database=PackageBuilderCommandStore;Integrated Security=SSPI;"
      providerName="System.Data.SqlClient" xdt:Transform="Replace" xdt:Locator="Match(name)"/>
  <add name="Lightstone"
       connectionString="Data Source=.;Database=PackageBuilder;Integrated Security=SSPI;"
       providerName="System.Data.SqlClient" xdt:Transform="Replace" xdt:Locator="Match(name)"/>
  <add name="Rgt"
       connectionString="Data Source=.;Database=PackageBuilder;Integrated Security=SSPI;"
       providerName="System.Data.SqlClient" xdt:Transform="Replace" xdt:Locator="Match(name)"/>
  <add name="RgtVin"
       connectionString="Data Source=.;Database=PackageBuilder;Integrated Security=SSPI;"
       providerName="System.Data.SqlClient" xdt:Transform="Replace" xdt:Locator="Match(name)"/>

  <add name="workflow/dataprovider/queue" connectionString="host=localhost" xdt:Transform="Replace" xdt:Locator="Match(name)"/>
  <add name="NServiceBus/Transport" connectionString="host=localhost" xdt:Transform="Replace" xdt:Locator="Match(name)"/>
  <add name="lace/source/database/auto-car-stats" connectionString="Data Source=#{Auto_Carstats_ds};Initial Catalog=Auto_Carstats;Integrated Security=True;" xdt:Transform="Replace" xdt:Locator="Match(name)"/>
  <add name="lace/source/database/ls-corporate-auto" connectionString="Data Source=.;Initial Catalog=LS_Corporate_Auto;Integrated Security=True;" xdt:Transform="Replace" xdt:Locator="Match(name)"/>

</connectionStrings>