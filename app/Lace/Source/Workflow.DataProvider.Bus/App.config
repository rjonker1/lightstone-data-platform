<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/>
  </startup>
  <connectionStrings>
    <add name="workflow/dataprovider/queue" connectionString="host=localhost" />
    <add name="workflow/dataprovider/database" providerName="System.Data.SqlClient" connectionString="Data Source=.;Initial Catalog=Monitoring;Integrated Security=True;Connect Timeout=15;Encrypt=False;TrustServerCertificate=False" />
    <add name="workflow/transactions/database" providerName="System.Data.SqlClient" connectionString="Data Source=.;Initial Catalog=Billing;Integrated Security=True;Connect Timeout=15;Encrypt=False;TrustServerCertificate=False" />
    <add name="workflow/monitoring/database" providerName="System.Data.SqlClient" connectionString="Data Source=.;Initial Catalog=Monitoring;Integrated Security=True;Connect Timeout=15;Encrypt=False;TrustServerCertificate=False" />
  </connectionStrings>
  <appSettings>
    <add key="service/config/name" value="DEV_Lightstone_DataProvider_Bus"/>
    <add key="service/config/displayName" value="DEV - Lightstone: Workflow Data Provider Service"/>
    <add key="service/config/description" value="DEV - Lightstone: Workflow Data Provider Service"/>
  </appSettings>
</configuration>
