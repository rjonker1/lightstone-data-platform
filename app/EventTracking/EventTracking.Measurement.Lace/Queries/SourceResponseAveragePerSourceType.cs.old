
namespace EventTracking.Measurement.Lace.Queries
{
    public class SourceResponseAveragePerSourceType
    {
        public string Average { get; private set; }
        public string Source { get; private set; }

        public SourceResponseAveragePerSourceType(string average, string source)
        {
            Average = average;
            Source = source;
        }

        public override string ToString()
        {
            return string.Format("Source:  {0}, Avg Response Time: {1}", Source, Average);
        }

    }
}
