@{
    Layout = "_Layout.cshtml";
}
<body class="skin-green">
    <div class="wrapper">
        @Html.Partial("_Header.cshtml")
        @Html.Partial("_Sidebar.cshtml")

        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            <!-- Content Header (Page header) -->
            <section class="content-header">
                <h1>
                    Monitoring
                    <small>Data Provider Caching</small>
                </h1>
            </section>
            <!-- Main content -->
            <section class="content">
                <div ng-app="dataProviderCachingApp">
                    <div class="row">
                        <div ng-controller="CachingController">
                            <div class="col-md-4">
                                <div class="info-box" style="cursor: pointer;" ng-click="refresh();">
                                    <span class="info-box-icon bg-blue"><i class="fa fa-refresh"></i></span>
                                    <div class="info-box-content">
                                        <span class="info-box-text">Refresh</span>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-4">
                                <div class="info-box" style="cursor: pointer;" ng-click="clear();">
                                    <span class="info-box-icon bg-red"><i class="fa fa-trash-o"></i></span>
                                    <div class="info-box-content">
                                        <span class="info-box-text">Clear</span>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-4">
                                <div class="info-box" style="cursor: pointer;" ng-click="restart();">
                                    <span class="info-box-icon bg-green"><i class="fa fa-play"></i></span>
                                    <div class="info-box-content">
                                        <span class="info-box-text">Restart</span>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </section><!-- /.content -->
        </div><!-- /.content-wrapper -->
        @Html.Partial("_Footer.cshtml")

    </div>
    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.4/js/bootstrap.min.js"></script>
    <!-- Slimscroll -->
    <script src="~/assets/plugins/slimScroll/jquery.slimscroll.min.js" type="text/javascript"></script>
    <!-- FastClick -->
    <script src='~/assets/plugins/fastclick/fastclick.min.js'></script>

    <script src="~/assets/js/demo.js" type="text/javascript"></script>
    <script type="text/javascript" charset="utf-8" src="~/assets/js/json-formatter.js"></script>

    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.3.6/angular.min.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.3.6/angular-route.min.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.3.6/angular-animate.min.js"></script>
    <script src="~/assets/ang/angular-ui-bootstrap.js"></script>
    <script src="~/assets/ang/app/dataProviderCachingApp.js"></script>
</body>