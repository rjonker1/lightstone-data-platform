{
    "certificate": {
        "key": "certificates/jsreport.net.key",
        "cert": "certificates/jsreport.net.cert"
    },
    "connectionString": { "name": "neDB" },
    "httpPort": 8856,
    "blobStorage": "fileSystem",
    "cluster": {
      "numberOfWorkers": 2,
      "enabled":  false
    },
    "phantom": {
        "numberOfWorkers" : 2,
        "timeout": 180000,
        "allowLocalFilesAccess": true
    },
    "tasks": {
        "numberOfWorkers" : 2,
        "timeout": 10000
    },
    "scripts": {
      "timeout": 30000,
      "allowedModules": ["handlebars", "request-json", "feedparser", "request", "underscore", "constants", "sendgrid", "mandrill-api/mandrill"]
}
}
